<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>compiled-bundle-settings</artifactId>
    <groupId>cz.zcu.kiv.crce.crce-reactor.build</groupId>
    <version>1.0.0-SNAPSHOT</version>
    <relativePath>../poms/compiled/</relativePath>
  </parent>
  <groupId>cz.zcu.kiv.crce.webui.build</groupId>
  <artifactId>deployment</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>CRCE - Repository Web UI</name>
  <description>Component repository supporting compatibility evaluation, Generated using Pax-Construct</description>
  <organization>
    <name>ZÄŒU KIV</name>
    <url>http://www.kiv.zcu.cz</url>
  </organization>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Maven Repository Switchboard</name>
      <url>http://repo1.maven.org/maven2</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>cz.zcu.kiv.crce</groupId>
      <artifactId>crce-webui</artifactId>
      <version>1.0.0-SNAPSHOT</version>
      <type>war</type>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>cz.zcu.kiv.crce</groupId>
      <artifactId>crce-repository</artifactId>
      <version>1.0.0-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>cz.zcu.kiv.crce.crce-reactor.build</groupId>
        <artifactId>provision</artifactId>
        <version>1.0.0-SNAPSHOT</version>
        <type>pom</type>
        <scope>provided</scope>
        <optional>true</optional>
      </dependency>
      <dependency>
        <groupId>org.osgi</groupId>
        <artifactId>osgi_R4_core</artifactId>
        <version>1.0</version>
        <scope>provided</scope>
        <optional>true</optional>
      </dependency>
      <dependency>
        <groupId>org.osgi</groupId>
        <artifactId>osgi_R4_compendium</artifactId>
        <version>1.0</version>
        <scope>provided</scope>
        <optional>true</optional>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <properties>
    <bundle.symbolicName>cz.zcu.kiv.crce.webui</bundle.symbolicName>
    <org.osgi.service.http.port>8090</org.osgi.service.http.port>
    <org.osgi.service.http.port.secure>8443</org.osgi.service.http.port.secure>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <bundle.namespace>cz.zcu.kiv.crce.webui</bundle.namespace>
  </properties>
</project>
